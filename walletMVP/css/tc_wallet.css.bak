/* 버튼그룹 */
.btn_group {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
      -ms-flex-direction: column;
          flex-direction: column;
  -webkit-box-pack: justify;
      -ms-flex-pack: justify;
          justify-content: space-between;
  width: 100%;
  gap: 10px;
}

.btn_group div {
  width: 100%;
}

.btn_group div button {
  height: 3em;
  border-width: 1px;
  border-style: solid;
  border-radius: 0.5em;
  border-color: transparent;
  color: #fff;
}

.btn_group div button span {
  font-size: 0.875em;
  font-weight: 500;
}

.btn_group div button.prev,
.btn_group div button.cancel {
  color: var(--main-color);
  font-weight: 400;
}

.btn_group div button.next {
  background-color: var(--main-color);
}

.btn_group div button.create {
  background-color: var(--main-color);
}

.btn_group div button.recovery {
  color: var(--main-color);
}

.btn_group div button.recovery span {
  text-decoration: underline;
  font-size: 0.875em;
}

.btn_group div button.login {
  background-color: #f5f5f5;
  color: #666;
}

.btn_group div button.login[disabled] {
  background-color: #f5f5f5;
  color: #666;
}

.btn_group div button.login {
  background-color: var(--main-color);
  color: #fff;
}

.btn_group.white {
  margin-top: 1em;
}

.btn_group.white div button {
  height: 3em;
  background-color: #fff;
}

.btn_group.white div button.prev,
.btn_group.white div button.cancel {
  border-color: #ccc;
  color: #999;
}

.btn_group.white div button.next {
  border-color: var(--main-color);
  color: var(--main-color);
}

/* bottom chk */
.btmChk {
  padding-bottom: 1.5em;
}

.btmChk label {
  position: relative;
}

.btmChk label input {
  position: absolute;
  opacity: 0;
}

.btmChk label span.checkmark {
  display: inline-block;
  position: absolute;
  top: 2px;
  left: 0;
  width: 20px;
  height: 20px;
  background-color: #fff;
  border: 1px solid #e3e3e3;
  border-radius: 100%;
}

.btmChk label span.checkmark::before {
  content: "";
  display: block;
  position: absolute;
  left: 50%;
  top: 50%;
  width: 10px;
  height: 7px;
  border-width: 0 0 1px 1px;
  border-style: solid;
  border-color: #e3e3e3;
  -webkit-transform: translate(-50%, -75%) rotate(-45deg);
          transform: translate(-50%, -75%) rotate(-45deg);
  -webkit-transform-origin: 50% 50%;
          transform-origin: 50% 50%;
}

.btmChk label input:checked + span.checkmark {
  background-color: var(--main-color);
  border-color: var(--main-color);
}

.btmChk label input:checked + span.checkmark::before {
  border-color: #fff;
}

.btmChk label span.txt {
  display: block;
  padding-left: 28px;
  font-size: 16px;
  line-height: 24px;
  font-weight: 500;
  word-break: keep-all;
}
